	DllCall("user32.dll", "none", "CallWindowProc", "ptr", DllStructGetPtr($_DES_CodeBuffer) + $_DES_EncryptCBCOffset, _
														"ptr", DllStructGetPtr($DesCtx), _
														"ptr", DllStructGetPtr($DataBuffer), _
														"uint", $DataLen, _
														"ptr", DllStructGetPtr($IVBuffer))
